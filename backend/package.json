{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "DYNAMODB_ENDPOINT=http://localhost:8000 DYNAMODB_TABLE=issues ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "ts-node src/scripts/seed-dynamodb.ts",
    "dynamodb:start": "./scripts/start-dynamodb-local.sh",
    "dynamodb:create-table": "ts-node src/scripts/create-dynamodb-table.ts",
    "dynamodb:setup": "npm run dynamodb:start && sleep 3 && npm run dynamodb:create-table",
    "deploy": "npm run build && npx serverless deploy",
    "deploy:dev": "npm run build && npx serverless deploy --stage dev",
    "deploy:staging": "npm run build && npx serverless deploy --stage staging",
    "deploy:prod": "npm run build && npx serverless deploy --stage production",
    "deploy:info": "npx serverless info",
    "deploy:logs": "npx serverless logs -f api --tail",
    "deploy:remove": "npx serverless remove",
    "deploy:remove:dev": "npx serverless remove --stage dev",
    "deploy:remove:staging": "npx serverless remove --stage staging",
    "deploy:remove:prod": "npx serverless remove --stage production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0",
    "@types/multer": "^2.0.0",
    "csv-parse": "^6.1.0",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "serverless-http": "^3.2.0",
    "uuid": "^11.0.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^30.2.0",
    "serverless": "^3.40.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
